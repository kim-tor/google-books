{
  "name": "google-books",
  "version": "1.0.0",
  "description": "Google Book Search",
  "main": "server.js",
  "scripts": {
    "start:local": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "start": "npm run start:prod"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.21.4",
    "concurrently": "^6.2.1",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.13.9",
    "react-router-dom": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kim-tor/google-books.git"
  },
  "bugs": {
    "url": "https://github.com/kim-tor/google-books/issues"
  },
  "homepage": "https://github.com/kim-tor/google-books#readme"
}
